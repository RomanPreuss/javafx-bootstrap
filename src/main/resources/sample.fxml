<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="BOTTOM_RIGHT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="sample.Controller">
    <children>
        <HBox fillHeight="false">
            <TextField fx:id="someTextField" prefWidth="300.0"/>
            <Button fx:id="fileChooserButton" mnemonicParsing="false" onAction="#handleChooseFileAction"
                    text="Durchsuchen"/>
            <padding>
                <Insets bottom="5.0"/>
            </padding>
        </HBox>
        <TableView fx:id="tableView">
            <columns>
                <TableColumn prefWidth="75.0" text="C1"/>
                <TableColumn prefWidth="75.0" text="C2"/>
            </columns>
        </TableView>
        <Button fx:id="someButton" onAction="#handleSomeAction" mnemonicParsing="false" text="Some Button"/>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
